package Algorithms;

public class EightQueens {
	
	//Main
	public static void main (String[] args) {
		
		System.out.println(Board(1,1));
		//call board -> generate
	}
	
	
	//must be able to
	//add queen
	public static void Board(int row, int column) {
		
		int[][] board = new int[8][8];
		
		board[row][column] = 1;
		
		for (int i = 0; i < 8; i++) {
			for (int j = 0; j < 8; j++) {
				System.out.print(board[i][j]);
			}
			System.out.println();
		}
		//return new board
	}
	
